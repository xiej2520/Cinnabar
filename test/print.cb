fun main() {
  let s = "abcde"
  let a = 45
  let b = 39
  
  // print an int, double, char, string
  __print(a)
  __println(b)
  __println(4.59380)
  __print(s)
  __println(s)
  __print("ABC\n")
  
  // print a bool
  __print(true)
  __println(false)
  __println("{} {}", true, false)

  // print a formatted string
  __print("{}, {}\n", s, a + b)
  __println("|\\{}\n{}{{}}xz|", s, a + b)
  
  // check escape sequences
  __print("%%")
  __println("%%{}%%%{}%", true, false)
  __print("\n\\\t\'\"\"{}\\{}\\", 1, false)

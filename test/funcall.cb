fun fib(i i32) i32 {
  if i <= 1 { return i; }
  return fib(i - 1) + fib(i - 2)
  // return if i <= 1 { i } else { fib(i - 1) + fib(i - 2) }
}
fun fib2(i i32) i32 {
  return if i <= 1 { i } else { fib2(i - 1) + fib2(i - 2) }
}

